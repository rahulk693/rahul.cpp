#include<iostream>
using namespace std;

class student{
    private:
    int rollno;
    int marks;
    public:
    void setstudent(){
        cout<<"Enter RollNO and Marks:-"<< endl;
        cin>>rollno>>marks;
    }
    void display(){
        cout<<"Enter ROll NO:-"<<rollno<< endl;
        cout<<"Enter Marks:-"<<marks<< endl;
    }
};
int main(){
    int n;
    cout<<"Number Of Students:-"<< endl;
    cin>>n;
    student s[n];
    cout<<"Enter Student Details:-"<< endl;
    for(int i=0;i<n;i++){
        s[i].setstudent();
    }
    for(int i=0;i<n;i++){
        s[i].display();
    }
    return 0;
}
