#include<iostream>
#include<set>
using namespace std;

void swap(int a,int b){
    int temp=a;
    a=b;
    b=temp;
    cout<<"a after swap:"<<a<< endl;
    cout<<"b after swap:"<<b<< endl;
}
int main(){
    int x,y;
    cout<<"enter value of a and b:-"<< endl;
    cin>>x>>y;
    cout<<"a before swap:"<<x<< endl;
    cout<<"b before swap:"<<y<< endl;
    swap(x,y);
    return 0;
